{
  "name": "design-token-toolkit",
  "sources": {
    "dir": "src/core",
    "subdirs": true
  },
  "package-specs": {
    "module": "esmodule",
    "in-source": true
  },
  "suffix": ".res.mjs",
  "bs-dependencies": [
    "@rescript/core"
  ],
  "bsc-flags": [
    "-open RescriptCore"
  ],
  "gentypeconfig": {
    "module": "esmodule",
    "moduleResolution": "node",
    "generatedFileExtension": ".gen.ts",
    "shims": {},
    "debug": {
      "all": false,
      "basic": false
    }
  },
  "warnings": {
    "error": "+101"
  },
  "namespace": false
}